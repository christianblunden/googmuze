<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon
   id="plugin.audio.googmuze"
   name="GoogMuze"
   version="0.0.5"
   provider-name="xianralph">
  <requires>
    <import addon="xbmc.python"                 version="2.1.0"/>
    <import addon="xbmc.gui"                    version="4.0.0"/>
    <import addon="xbmc.json"                   version="6.0.0"/>
    <import addon="xbmc.addon"                  version="12.0.0"/>
    <import addon="xbmc.metadata"               version="2.1.0"/>
    <import addon="script.module.validictory"   version="0.9.0"/>
    <import addon="script.module.decorator"     version="3.4.0"/>
    <import addon="script.module.requests"      version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="googmuze.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Simple interface to Google Play Music</summary>
    <description lang="en">description here</description>
    <disclaimer lang="en">There is NO support for this plugin. For bugs, requests or general questions you are on your own. Use at your own risk</disclaimer>
    <platform>all</platform>
  </extension>
</addon>